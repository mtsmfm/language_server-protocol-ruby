module LanguageServer
  module Protocol
    module Interface
      class DocumentOnTypeFormattingRegistrationOptions
        def initialize: (document_selector: untyped, first_trigger_character: String, ?more_trigger_character: Array[String]) -> void

        @attributes: Hash[Symbol, untyped]
        attr_reader attributes: Hash[Symbol, untyped]

        %a{pure}
        def document_selector: () -> untyped

        %a{pure}
        def first_trigger_character: () -> String

        %a{pure}
        def more_trigger_character: () -> Array[String]?

        def to_hash: () -> Hash[Symbol, untyped]

        def to_json: (*untyped) -> String
      end
    end
  end
end
