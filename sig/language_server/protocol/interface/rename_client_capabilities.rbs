module LanguageServer
  module Protocol
    module Interface
      class RenameClientCapabilities
        def initialize: (?dynamic_registration: bool, ?prepare_support: bool, ?prepare_support_default_behavior: untyped, ?honors_change_annotations: bool) -> void

        @attributes: Hash[Symbol, untyped]
        attr_reader attributes: Hash[Symbol, untyped]

        %a{pure}
        def dynamic_registration: () -> bool?

        %a{pure}
        def prepare_support: () -> bool?

        %a{pure}
        def prepare_support_default_behavior: () -> untyped

        %a{pure}
        def honors_change_annotations: () -> bool?

        def to_hash: () -> Hash[Symbol, untyped]

        def to_json: (*untyped) -> String
      end
    end
  end
end
