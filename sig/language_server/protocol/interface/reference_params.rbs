module LanguageServer
  module Protocol
    module Interface
      class ReferenceParams
        def initialize: (text_document: untyped, position: untyped, ?work_done_token: untyped, ?partial_result_token: untyped, context: untyped) -> void

        @attributes: Hash[Symbol, untyped]
        attr_reader attributes: Hash[Symbol, untyped]

        %a{pure}
        def text_document: () -> untyped

        %a{pure}
        def position: () -> untyped

        %a{pure}
        def work_done_token: () -> untyped

        %a{pure}
        def partial_result_token: () -> untyped

        %a{pure}
        def context: () -> untyped

        def to_hash: () -> Hash[Symbol, untyped]

        def to_json: (*untyped) -> String
      end
    end
  end
end
